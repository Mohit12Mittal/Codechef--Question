#include<stdio.h>
int main()
{
    int num;
    scanf("%d",&num);
    for(int i=0;i<num;i++)
    {
        int n;
        scanf("%d",&n);
        char ch[10];
        int sum1=0,sum2=0;
        for(int i=0;i<n;i++)
        {
            scanf("%s",ch);
            if(ch[0]=='S')
            {
                sum1++;
            }
            else
            {
                sum2++;
            }
        }
        printf("%d %d\n",sum1,sum2);
    }
    return 0;
}

